<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/resultstyle.css">
    <style>
        h1 {
            text-align: center;
            color: rgba(134, 95, 11, 0.913);
            margin-bottom: 30px;
        }
        h3 {
            text-align: center;
            font-size: 25px;
            color: rgba(134, 95, 11, 0.913);
        }
    </style>
</head>
<body>

 <h1>FINAL RESULT</h1>
<div class="win">WINNER IS</div>
<div class="table1">
 <table class="table2"> 
    <tbody>
        <tr>
    <th>SLNO</th>
    <th>NAME</th>
    <th>PROFILE</th>
    <th>SYMBOL</th>
    <th>VOTE COUNT</th>
  </tr>
        {{#each winner}}
            <tr>
                <td>{{this.idno}}</td>
                  <td>{{this.name}}</td>
                <td><img src="data:image/png;base64,{{this.image}}"  width="110px" height="100px"></td>
                <td><img src="data:image/png;base64,{{this.symbol}}"  width="110px" height="100px"></td>
                <td>  {{this.votes}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

        </div>

<h3>Candidate List</h3>
<div class="table">
 <table>
    <tbody>
        <tr>
    <th>SLNO</th>
    <th>NAME</th>
    <th>PROFILE</th>
    <th>SYMBOL</th>
    <th>VOTE COUNT</th>
  </tr>
        {{#each data}}
            <tr>
                <td>{{this.idno}}</td>
                  <td>{{this.name}}</td>
                <td><img src="data:image/png;base64,{{this.image}}"  width="110px" height="100px"></td>
                <td><img src="data:image/png;base64,{{this.symbol}}"  width="110px" height="100px"></td>
                <td>  {{this.votes}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

        </div>

<script>
    if(window.history.replaceState){
        window.history.replaceState(null,null,window.location.href);
    }
</script>

</body>
</html>
